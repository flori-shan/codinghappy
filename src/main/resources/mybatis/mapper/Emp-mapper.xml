<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.nihility.dao.EmpMapper">
	<select id="selectEmpByEmpno" parameterType="java.lang.Integer" resultType="cn.nihility.bean.EmpBean">
		select * from emp where empno = #{id}
	</select>
	
    <select id="selectEmpsByDeptno" parameterType="java.lang.Integer" resultType="cn.nihility.bean.EmpBean">
        select * from emp where deptno = #{id}
    </select>
    
    <select id="selectEmpsMapByDeptno" parameterType="java.lang.Integer" resultType="cn.nihility.bean.EmpBean">
        select * from emp where deptno = #{id}
    </select>
    
    
</mapper>