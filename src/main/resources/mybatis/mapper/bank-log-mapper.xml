<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.nihility.spring.mybatis.dao.BankLogDao">

    <insert id="insertByObj" parameterType="BankLog" keyProperty="id" useGeneratedKeys="true">
        INSERT INTO bank_log (name, status) values (#{name}, #{status})
    </insert>

</mapper>